{
  "routes": [
    {
      "route": "/assets/*.js",
      "headers": {
        "content-type": "application/javascript; charset=utf-8"
      }
    },
    {
      "route": "/assets/*.mjs",
      "headers": {
        "content-type": "application/javascript; charset=utf-8"
      }
    },
    {
      "route": "/assets/*.css",
      "headers": {
        "content-type": "text/css; charset=utf-8"
      }
    },
    {
      "route": "/*.js",
      "headers": {
        "content-type": "application/javascript; charset=utf-8"
      }
    },
    {
      "route": "/*.mjs",
      "headers": {
        "content-type": "application/javascript; charset=utf-8"
      }
    },
    {
      "route": "/*.css",
      "headers": {
        "content-type": "text/css; charset=utf-8"
      }
    },
    {
      "route": "/*",
      "serve": "/index.html",
      "statusCode": 200
    }
  ],
  "navigationFallback": {
    "rewrite": "/index.html"
  },
  "mimeTypes": {
    ".js": "application/javascript",
    ".mjs": "application/javascript",
    ".css": "text/css",
    ".json": "application/json",
    ".woff": "font/woff",
    ".woff2": "font/woff2"
  },
  "globalHeaders": {
    "X-Content-Type-Options": "nosniff"
  }
}
